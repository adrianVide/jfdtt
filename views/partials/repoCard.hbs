<div class="card">
    <img class="card-img" src="https://loremflickr.com/320/180/coding?random={{this.id}}" alt="coding">
    <div class="card-body">
        <h3 class="card-title">{{this.name}}</h3>
        <p class="card-text">{{this.description}}</p>
        <form action="{{this.html_url}}" , method='GET' target="_blank" rel="nofollow">
            <button class="card-button">Link to Repo</button> </form>
    </div>
</div>